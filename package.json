    "@types/passport": "^1.0.1",
    "@types/passport-jwt": "^3.0.2",
    "@types/passport-local": "^1.0.33",
    "@types/uuid": "^3.4.5",
    "nodemon": "^1.19.1",
    "rimraf": "^2.6.3",
